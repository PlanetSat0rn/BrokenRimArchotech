<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <GeneDef>
    <defName>ToughGene</defName>
    <label>tough</label>
    <labelShortAdj>tough</labelShortAdj>
    <description>Those who have this gene has thick skin, dense flesh, and durable bones. They take much less damage than other people from the same blows. They are extremely hard to kill.</description>
    <iconPath>UI/Icons/Genes/Gene_Tough</iconPath>
    <displayCategory>Pain</displayCategory>
    <displayOrderInCategory>60</displayOrderInCategory>
    <forcedTraits>
      <li>
        <def>Tough</def>
      </li>
    </forcedTraits>
    <biostatMet>-3</biostatMet>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>tough</symbol></li>
        <li><symbol>stout</symbol></li>
        <li><symbol>rough</symbol></li>
        <li><symbol>lump</symbol></li>
        <li><symbol>bruise</symbol></li>
        <li><symbol>stiff</symbol></li>
        <li><symbol>hard</symbol></li>
        <li><symbol>vigo</symbol></li>
      </prefixSymbols>
    </symbolPack>
  </GeneDef>
  
  <GeneDef>
    <defName>ArchotechMetabolism</defName>
    <label>archotech metabolism</label>
    <labelShortAdj>archotech-metabolic</labelShortAdj>
    <description>Carriers of this gene have even more special archites in their cells that facilitate and optimize metabolism. This improves overall genetic and metabolic quality.</description>
    <iconPath>UI/Icons/Genes/Gene_ArchiteMetabolism</iconPath>
    <displayCategory>Archite</displayCategory>
    <marketValueFactor>2</marketValueFactor>
    <biostatCpx>12</biostatCpx>
    <biostatMet>12</biostatMet>
    <biostatArc>8</biostatArc>
  </GeneDef>
  
  <GeneDef>
    <defName>SuperiorMetabolism</defName>
    <label>superior metabolism</label>
    <labelShortAdj>superior-metabolic</labelShortAdj>
    <description>Carriers of this gene have a lot more special archites in their cells that facilitate and optimize metabolism. This improves overall genetic and metabolic quality.</description>
    <iconPath>UI/Icons/Genes/Gene_ArchiteMetabolism</iconPath>
    <displayCategory>Archite</displayCategory>
    <marketValueFactor>4</marketValueFactor>
    <biostatCpx>32</biostatCpx>
    <biostatMet>32</biostatMet>
    <biostatArc>18</biostatArc>
  </GeneDef>

  <GeneDef>
    <defName>ArchotechOrigins</defName>
    <label>archotech origins</label>
    <labelShortAdj>archotech-origins</labelShortAdj>
    <description>This gene holder is of Archotech origins. They are worshipped, respected and often sell for a lot.</description>
    <iconPath>SpecializedXenotypes/GeneIcons/Unknown</iconPath>
    <displayCategory>Archite</displayCategory>
	<statOffsets>
      <PawnBeauty>5</PawnBeauty>
	  <MarketValue>8000</MarketValue>
    </statOffsets>
    <biostatCpx>20</biostatCpx>
	<biostatArc>999</biostatArc>
	    <symbolPack>
      <prefixSymbols>
        <li><symbol>archo</symbol></li>
      </prefixSymbols>
    </symbolPack>
  </GeneDef>
  
    <GeneDef  MayRequire="Ludeon.RimWorld.Royalty">
    <defName>SX_InnatePsylink</defName>
    <label>innate psylink</label>
    <labelShortAdj>innate-psylink</labelShortAdj>
    <description>This gene holder has the ability to use psycasts.</description>
    <iconPath>SpecializedXenotypes/GeneIcons/Unknown</iconPath>
    <displayCategory>Psychic</displayCategory>
<modExtensions>
	<li Class="VanillaGenesExpanded.GeneExtension">
		<hediffsToBodyParts>
			<li>
				<hediff>PsychicAmplifier</hediff>
				<bodyparts>
					<li>Brain</li>
				</bodyparts>
			</li>
		</hediffsToBodyParts>
	</li>
</modExtensions>
    <biostatCpx>20</biostatCpx>
	<biostatMet>-3</biostatMet>
	<biostatArc>3</biostatArc>
  </GeneDef>
</Defs>